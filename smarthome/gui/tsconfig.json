{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "CommonJS",
    "target": "ES6",
    "jsx": "react",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,

    "lib": [
      "ES2023",
      "DOM",
      "ScriptHost",
      "esnext.disposable"
    ],
    "types": [
      "node"
    ]
  },
  "include": [
    "src/**/*"
  ]
}