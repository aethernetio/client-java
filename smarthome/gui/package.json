    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^4.1.0",
    "js-base64": "^3.7.8"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/jest": "^29.5.14",
    "aether-protocol-generator": "file:aether-protocol-generator-1.0.0.tgz",
    "html-inline-script-webpack-plugin": "^3.2.1",
    "html-webpack-plugin": "^5.6.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "js-yaml": "^4.1.0",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "ws": "^8.18.3"
  }
}
